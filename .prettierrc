{
  "semi": true,
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "quoteProps": "consistent",
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": ["*.tsx"],
      "options": {
        "jsxSingleQuote": false
      }
    }
  ]
}
